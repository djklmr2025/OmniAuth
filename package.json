
{
  "name": "omniauth-secure",
  "version": "1.1.0",
  "main": "electron-main.js",
  "description": "Autenticador 2FA seguro multiplataforma con IA y NIP.",
  "author": "OmniAuth Dev",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "electron-dev": "electron .",
    "dist": "electron-builder build --win --portable"
  },
  "dependencies": {
    "otpauth": "^9.5.0",
    "lucide-react": "^0.564.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "@google/genai": "^1.41.0"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "build": {
    "appId": "com.omniauth.secure",
    "productName": "OmniAuth Secure",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "portable",
      "icon": "icon.ico",
      "publisherName": "OmniAuth Security"
    },
    "files": [
      "build/**/*",
      "electron-main.js",
      "package.json"
    ]
  }
}
